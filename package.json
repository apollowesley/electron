{
	"name": "xea-cli-electron-service",
	"version": "1.0.0",
	"subversion": "2020",
	"main": "index.js",
	"license": "MIT",
	"scripts": {
		"kill": "node package/index kill",
		"open": "node package/index openApp",
		"start": "cross-env NODE_ENV=development node package/index views serve",
		"start:views": "cross-env NODE_ENV=development node package/index views",
		"start:serve": "cross-env NODE_ENV=development node package/index serve",
		"build": "cross-env NODE_ENV=production node package/index views serve",
		"build:views": "cross-env NODE_ENV=production node package/index views",
		"build:serve": "cross-env NODE_ENV=production node package/index serve",
		"postbuild": "electron-builder --win --x64",
		"eslint": "node_modules/.bin/eslint . --color --ext .ts,.tsx",
		"lint:fix": "node_modules/.bin/eslint . --fix --ext .ts,.tsx",
		"lint:staged": "node_modules/.bin/eslint --color",
		"postlint:staged": "node_modules/.bin/tsc -p ./tsconfig.json"
	},
	"dependencies": {
		"antd": "^4.2.5",
		"axios": "^0.19.2",
		"core-js": "^3.6.5",
		"dayjs": "^1.8.18",
		"fs-extra": "^9.0.0",
		"history": "^4.10.1",
		"koa": "^2.12.0",
		"koa-bodyparser": "^4.3.0",
		"koa-compress": "^4.0.1",
		"koa-favicon": "^2.1.0",
		"koa-router": "^8.0.8",
		"koa-static": "^5.0.0",
		"mobx": "^5.15.4",
		"mobx-react": "^6.2.2",
		"mobx-react-router": "^4.1.0",
		"nedb": "^1.8.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-loadable": "^5.5.0",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.10.0",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-decorators": "^7.10.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.10.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/preset-env": "^7.10.0",
		"@babel/preset-typescript": "^7.9.0",
		"@types/koa": "^2.11.3",
		"@types/koa-router": "^7.4.1",
		"@types/node": "^12.0.12",
		"@types/react": "^16.9.35",
		"@types/react-router-dom": "^5.1.5",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"colors": "^1.4.0",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"cssnext": "^1.8.4",
		"electron": "^9.0.0",
		"electron-builder": "^22.6.1",
		"error-overlay-webpack-plugin": "^0.4.1",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-react-app": "^5.2.1",
		"eslint-friendly-formatter": "^4.0.1",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-flowtype": "^4.7.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^2.5.0",
		"html-webpack-plugin": "^4.3.0",
		"husky": "^4.2.5",
		"json-schema": "^0.2.5",
		"lint-staged": "^10.2.6",
		"minimist": "^1.2.5",
		"npm-run-all": "^4.1.5",
		"postcss-comment": "^2.0.0",
		"postcss-flexbugs-fixes": "^4.2.1",
		"postcss-import": "^12.0.1",
		"precss": "^4.0.0",
		"prettier": "^2.0.5",
		"shelljs": "^0.8.4",
		"thread-loader": "^2.1.3",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.3",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0",
		"webpack-node-externals": "^1.7.2",
		"webpackbar": "^4.0.0"
	},
	"build": {
		"publish": [
			{
				"provider": "generic",
				"url": ""
			}
		],
		"asar": true,
		"appId": "com.xea.electron-service",
		"productName": "electron-template",
		"files": [
			"dist",
			"favicon.ico",
			"index.js",
			"xea.config.js",
			"!**/*.{lib,obj,pdb,cc,h,tlog,map,exp,gypi,vcxproj,filters,cpp,c,ts}"
		],
		"nodeGypRebuild": false,
		"extraFiles": [],
		"win": {
			"icon": "./favicon.ico",
			"requestedExecutionLevel": "asInvoker"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"perMachine": true,
			"language": 2052
		},
		"directories": {
			"output": "output"
		}
	},
	"window": {
		"center": true,
		"title": "",
		"show": true,
		"backgroundColor": "#f0f0f0",
		"minWidth": 1040,
		"minHeight": 768,
		"frame": true,
		"webPreferences": {
			"webSecurity": true,
			"nodeIntegration": true
		}
	},
	"engines": {
		"node": ">=8.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(ts|tsx)": "npm run lint:staged"
	}
}
