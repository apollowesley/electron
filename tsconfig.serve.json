{
  "extends": "./tsconfig",
  "compilerOptions": {
    "outDir": "dist/serve",
    "moduleResolution": "node",
    "skipDefaultLibCheck": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "suppressImplicitAnyIndexErrors": true,
    "types": [
      "node"
    ]
  },
  "include": [
    "serve",
    "global",
    "typings",
    "node_modules"
  ]
}